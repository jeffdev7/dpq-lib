<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
	  <PackageId>DPQ.lib</PackageId>
	  <Version>1.0.4</Version>
	  <Authors>Jeff</Authors>
	  <Description>Library for masking Personally Identifiable Information (PII) using DPQ - Data Mesh</Description>
	  <PackageTags>datamesh;masking;cpf;lgpd;gdpr;privacy</PackageTags>
